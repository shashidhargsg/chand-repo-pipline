pipeline {
  agent any
   tools{
     maven 'Maven_home'
     }
    
        stages {
            stage ('git'){
                steps {
                    git 'https://github.com/shashidhargsg/chand-repo-pipline.git'
                }
            }
     stage ('Build') {
       steps {
           sh "mvn clean package"
       }
      }
  }
  }
